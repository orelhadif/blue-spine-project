{
  "$schema": "https://json.schemastore.org/angular.json",
  "version": 1,
  "projects": {
    "claims-recon-client": {
      "projectType": "application",
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/claims-recon-client",
            "index": "src/index.html",
            "browser": "src/main.ts",
            "polyfills": ["zone.js"],
            "tsConfig": "tsconfig.app.json",
            "assets": ["src/favicon.ico", "src/assets"],
            "styles": ["src/styles.less"],
            "inlineStyleLanguage": "less"
          },
          "configurations": {
            "development": {
              "optimization": false,
              "aot": true,
              "sourceMap": true
            },
            "production": {
              "optimization": true,
              "aot": true,
              "sourceMap": false
            }
          },
          "defaultConfiguration": "development"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "development": {
              "buildTarget": "claims-recon-client:build:development"
            },
            "production": {
              "buildTarget": "claims-recon-client:build:production"
            }
          },
          "defaultConfiguration": "development"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": ["zone.js", "zone.js/testing"],
            "tsConfig": "tsconfig.spec.json",
            "assets": ["src/favicon.ico", "src/assets"],
            "styles": ["src/styles.less"],
            "inlineStyleLanguage": "less",
            "scripts": []
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "e88e8d63-6254-41a5-8912-eae205e82c41"
  }
}
